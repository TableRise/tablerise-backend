        "@types/bcrypt": "5.0.0",
        "@types/chai": "4.3.9",
        "@types/cookie-parser": "1.4.6",
        "@types/cookie-session": "2.0.48",
        "@types/cors": "2.8.13",
        "@types/expect": "24.3.0",
        "@types/express": "4.17.17",
        "@types/express-session": "1.17.7",
        "@types/jsonwebtoken": "9.0.3",
        "@types/mocha": "10.0.3",
        "@types/module-alias": "2.0.1",
        "@types/multer": "1.4.11",
        "@types/node": "20.1.1",
        "@types/node-cron": "3.0.11",
        "@types/nodemailer": "6.4.9",
